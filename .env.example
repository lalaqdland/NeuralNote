# NeuralNote 环境变量配置模板
# 复制此文件为 .env 并填入实际的配置值

# ==================== 应用配置 ====================
ENVIRONMENT=development
DEBUG=true
SECRET_KEY=your-secret-key-here-change-in-production

# ==================== 数据库配置 ====================
POSTGRES_USER=neuralnote
POSTGRES_PASSWORD=neuralnote_dev_password
POSTGRES_DB=neuralnote_dev
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
DATABASE_URL=postgresql://neuralnote:neuralnote_dev_password@localhost:5432/neuralnote_dev

# ==================== Redis 配置 ====================
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_URL=redis://localhost:6379/0

# ==================== AI 服务配置 ====================
OPENAI_API_KEY=sk-your-openai-api-key-here
DEEPSEEK_API_KEY=sk-your-deepseek-api-key-here

# ==================== OCR 服务配置 ====================
BAIDU_OCR_API_KEY=your-baidu-ocr-api-key
BAIDU_OCR_SECRET_KEY=your-baidu-ocr-secret-key

# ==================== 对象存储配置 ====================
ALIYUN_ACCESS_KEY_ID=your-aliyun-access-key-id
ALIYUN_ACCESS_KEY_SECRET=your-aliyun-access-key-secret
